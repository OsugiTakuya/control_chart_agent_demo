# OpenAI設定
OPENAI_API_KEY=sk-xxxx
# エージェントごとのモデル & デプロイ名
ROUTER_MODEL=gpt-4o-mini
PLANNER_MODEL=gpt-4o-mini
EXECUTOR_MODEL=gpt-4o-mini
SUMMARIZER_MODEL=gpt-4o
RESPONDER_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small

# Azure設定（両方設定されていればAzure優先）
AZURE_OPENAI_API_KEY=xxxx
AZURE_OPENAI_ENDPOINT=https://your-azure.openai.azure.com/
AZURE_OPENAI_API_VERSION=2025-04-01-preview
# エージェントごとのモデル & デプロイ名
ROUTER_AZURE_DEPLOYMENT=gpt-4o-mini
PLANNER_AZURE_DEPLOYMENT=gpt-4o-mini
EXECUTOR_AZURE_DEPLOYMENT=gpt-4o-mini
SUMMARIZER_AZURE_DEPLOYMENT=gpt-4o
RESPONDER_AZURE_DEPLOYMENT=gpt-4o-mini
EMBEDDING_AZURE_DEPLOYMENT=text-embedding-3-small
